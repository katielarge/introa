<script>
    
    function gasMile(gas, startMile, endMile) {
        
        var difference = startMile - endMile;
        var answer = difference / gas;
        console.log(answer);
    }
    
   var endNumber = prompt("What was your car's final odometer reading?");
   
   gasMile(12,10000,parseInt(endNumber));    
    
    
</script> 
