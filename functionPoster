<script>
    
    function GPA (grade1, grade2, grade3, grade4,grade5, grade6) {
        var answer = (grade1 + grade2 + grade3 + grade4 + grade5 + grade6)/credits
        console.log(answer)
    }
    
    var a = prompt("Submit math grade average for all years in for the number equivalent - A+=4.33 A=4.0 A-=3.7 B+=3.3 B=3.0 B-=2.7 C+=2.3 C=2.0 C-=1.7 D=1.0 D-/F=0.0")
    var b = prompt("Submit english grade average for all years in for the number equivalant - A+=4.33 A=4.0 A-=3.7 B+=3.3 B=3.0 B-=2.7 C+=2.3 C=2.0 C-=1.7 D=1.0 D-/F=0.0")
    var c = prompt("Submit science grade average for all years in for the number equivalant - A+=4.33 A=4.0 A-=3.7 B+=3.3 B=3.0 B-=2.7 C+=2.3 C=2.0 C-=1.7 D=1.0 D-/F=0.0")
    var d = prompt("Submit history average for all years in for the number equivalant - A+=4.33 A=4.0 A-=3.7 B+=3.3 B=3.0 B-=2.7 C+=2.3 C=2.0 C-=1.7 D=1.0 D-/F=0.0")
    var e = prompt("Submit language average for all years in for the number equivalant - A+=4.33 A=4.0 A-=3.7 B+=3.3 B=3.0 B-=2.7 C+=2.3 C=2.0 C-=1.7 D=1.0 D-/F=0.0")
    var f = prompt("Submit PE average for all years in for the number equivalant - A+=4.33 A=4.0 A-=3.7 B+=3.3 B=3.0 B-=2.7 C+=2.3 C=2.0 C-=1.7 D=1.0 D-/F=0.0")
    var credits = prompt("How many credits did you earn in your high school career? //full year 6 blocks=1.0 credits // full year 3 blocks=0.5 credits // semester 6 blocks=0.5 credits // semester 3 blocks=0.25// *add up each credit for your six classes and write the answer below*")
    
    GPA(parseInt(a) * 4, parseInt(b) * 4, parseInt(c) * 4, parseInt(d) * 4, parseInt(e) * 4, parseInt(f) * 4)
        
        
    
    
</script>     
